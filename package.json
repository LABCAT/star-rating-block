{
  "name": "star-rating-block",
  "version": "1.0.0",
  "description": "Custom web component for star ratings",
  "type": "module",
  "main": "./dist/star-rating-block.es.js",
  "module": "./dist/star-rating-block.es.js",
  "types": "./dist/RatingBlock.d.ts",
